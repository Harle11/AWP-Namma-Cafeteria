<main id="main">
  <div class="container">
    <div class="row" id="menucards">
      <% for( let i = 0; i < dish_types.length; i++ ) { %>
        <div class="col-sm-4">
          <div class="card card-bg">
            <a href="/manager/submenu?type=<%= durls[i] %> ">
              <div class="card-body" id="menu<%= durls[i] %>">
                <div class="card-title"><%= dish_types[i] %></div>
              </div>
            </a>
          </div>
        </div>
      <% } %>
    </div>
  </div>
  <div class="container d-flex justify-content-around" style="margin-top: 20px;"><!--Buttons-->
    <a href="/home" class="nav-btn">Back To Home</a>
  </div>
</main>