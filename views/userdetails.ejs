<main id="main">
  <div class="container">
    <div class="cartbox">
      <h1 style="text-align: center; color:#FFB03B; font-family: Satisfy, sans-serif;">User Details</h1>
      <% if (locals.LoadUserFailed) { %>
       <h5>Unable to get User details right now, try again later.</h5>
      <% } else { %>
        <div class="container" style="padding: 1% 0 3% 5%;">
          <p><span style="font-weight: 700;">Name: </span><%= Uname %></p>
          <p><span style="font-weight: 700;">Email: </span><%= Uemail %></p>
          <p><span style="font-weight: 700;">Credit Balance: </span><%= Ucredit %></p>
          <p style="margin-bottom: 0px;"><span style="font-weight: 700;">Quick Orders: </span><% if (!Uqck_orders) { %>No Quick orders set</p>
            <% } else { %>
            </p><ul>
              <% for( let index = 0; index < Uqck_orders.length; index++ ) { %>
                <li><%= Uqck_orders[index] %> </li>
              <% } %>
            </ul>
          <% } %>
        </div>
      <% } %>
    </div>
  </div>
  <div class="container d-flex justify-content-around" style="margin-top: 30px;"><!--Buttons-->
      <a href="/home" class="nav-btn">Back To Home</a>
  </div>
</main>