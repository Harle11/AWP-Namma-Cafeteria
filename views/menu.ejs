<main id="main">
  <div class="container"><!--Cards-->
    <div class="row" id="menucards">
      <% for( let i = 0; i < dishTypes.length; i++ ) { %>
        <div class="col-sm-4">
          <div class="card card-bg">
            <a href="/submenu?type=<%= durls[i] %> ">
              <div class="card-body" id="menu<%= durls[i] %>">
                <div class="card-title"><%= dishTypes[i] %></div>
              </div>
            </a>
          </div>
        </div>
      <% } %>
    </div>
  </div>
  <div class="container d-flex justify-content-around" style="margin-top: 30px;"><!--Buttons-->
    <a href="/home" class="nav-btn">Back To Home</a>
  </div>
</main>