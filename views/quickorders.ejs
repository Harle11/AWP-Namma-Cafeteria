<main id="main">
  <div class="container"><!--Cart-->
    <div class="cartbox">
      <h1 style="text-align: center; color:#FFB03B; font-family: Satisfy, sans-serif;">Quick Orders</h1>
      <% if (locals.LoadUserFailed) { %>
        <h5>Unable to get User details right now, try again later.</h5>
      <% } else if (Uqck_orders){ %>
        <table id="cart-table">
          <thead>
            <tr>
              <th>Item Name</th>
              <th valign="right">Price</th>
              <th valing="right">Order Again?</th>
            </tr>
          </thead>
          <tbody>
            <% for( let index = 0; index < Uqck_orders.length; index++ ) { %>
              <tr>
                <td><%= Uqck_orders[index] %></td>
                <td>â‚¹<%= orderprice[index] %> </td>
                <td><a href="#">Re-Order</a></td>
              </tr>
            <% } %>
          </tbody>
        </table>
      <% } else { %>
        <h6 style="padding: 1% 1% 3% 5%;">You have not set any quick orders</h6>
      <% } %>
    </div>
  </div>
  <div class="container d-flex justify-content-around" style="margin-top: 30px;"><!--Buttons-->
      <a href="/menu" class="nav-btn">Back To Menu</a>
  </div>
</main>