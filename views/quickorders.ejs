<body>
  <!-- ======= Header ======= -->
  <header id="header">
    <div id="mySidenav" class="sidenav">
      <div class="top">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()"><i class="fa fa-times" aria-hidden="true"></i></a>
        <a href="/userdetails">User Details and Balance</a>
